<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!-- Ate as Ingredient Thoughts -->
  <ThoughtDef>
    <defName>AteExtravagantMeal</defName>
    <showBubble>true</showBubble>
    <icon>Things/Mote/ThoughtSymbol/Food</icon>
    <durationDays>0.5</durationDays>
    <stackLimit>1</stackLimit>
    <stages>
      <li>
        <label>ate extravagant meal</label>
        <description>I... I have no words.. is.. is this what the gods eat? Do they eat like this on glitterworlds?</description>
        <baseMoodEffect>20</baseMoodEffect>
      </li>
    </stages>
  </ThoughtDef>
  <ThoughtDef>
    <defName>AteCannedMeal</defName>
    <showBubble>true</showBubble>
    <icon>Things/Mote/ThoughtSymbol/Food</icon>
    <durationDays>0.5</durationDays>
    <stackLimit>1</stackLimit>
    <stages>
      <li>
        <label>ate canned meal</label>
        <description>Eh.  Canned meals aren't as good as freshly cooked ones, but it could be worse.</description>
        <baseMoodEffect>-1</baseMoodEffect>
      </li>
    </stages>
  </ThoughtDef>
  <ThoughtDef>
    <defName>AteCannedMeatAsIngredient</defName>
    <showBubble>true</showBubble>
    <icon>Things/Mote/ThoughtSymbol/Food</icon>
    <durationDays>0.5</durationDays>
    <stackLimit>3</stackLimit>
