<Defs>
  <RecipeDef ParentName="RC2_CookJoyMealBase">
    <defName>RC2_MakeFruitPreservesBulk</defName>
    <label>make fruit preserves in bulk</label>
    <description>Uses sweets and fruit to preserve fruit.  Very tasty. Makes 25.</description>
    <jobString>Baking pastries.</jobString>
    <workAmount>800</workAmount>
    <recipeUsers>
      <li>RC2_Cookomatic</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>RC2_SweetsRaw</li>
            <li>RC2_SweetsProcessed</li>
          </categories>
        </filter>
        <count>1.25</count>
      </li>
      <li>
        <filter>
          <categories>
            <li>RC2_FruitsRaw</li>
          </categories>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
    <products>
      <RC2_FruitPreserves>25</RC2_FruitPreserves>
    </products>
    <fixedIngredientFilter>
      <categories>
        <li>RC2_SweetsRaw</li>
        <li>RC2_SweetsProcessed</li>
        <li>RC2_FruitsRaw</li>
      </categories>
    </fixedIngredientFilter>
    <defaultIngredientFilter>
      <categories>
        <li>RC2_SweetsRaw</li>
        <li>RC2_SweetsProcessed</li>
        <li>RC2_FruitsRaw</li>
      </categories>
    </defaultIngredientFilter>
    <researchPrerequisite>PackagedSurvivalMeal</researchPrerequisite>
  </RecipeDef>
